.data-tables-style__block
    #da-table
        .da-table__main-title
            span Card Heading
            .material-icons filter_list
                .rippleJS
            .material-icons more_vert
                .rippleJS
        .da-table__selected-items
            span#da-table__sum 0 selected items
            .material-icons delete
                .rippleJS
            .material-icons more_vert
                .rippleJS

        label.da-table__header(for="tableHeader")
            input#tableHeader.da-checkbox(type='checkbox' name="notifications")
            label.da-checkbox-label(for='tableHeader')
                span.da-checkbox__logo
                    svg(width='12px' height='10px' viewBox='0 0 12 10')
                        polyline(points='1.5 6 4.5 9 10.5 1')
            .da-table__input-content
                span Column Heading
                span Calories
                span Fat (g)
                span Carbs (g)
                span Protein (g)
